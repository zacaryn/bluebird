{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:PutItem",
				"dynamodb:GetItem",
				"dynamodb:Query",
				"dynamodb:UpdateItem",
				"dynamodb:DeleteItem",
				"dynamodb:Scan"
			],
			"Resource": "arn:aws:dynamodb:*:*:table/bluebirdmortgage-inquiries"
		},
		{
			"Effect": "Allow",
			"Action": [
				"cognito-idp:InitiateAuth"
			],
			"Resource": "*"
		}
	]
} 